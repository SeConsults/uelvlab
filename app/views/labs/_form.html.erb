<% if @lab.errors.any? %>
 <ul>
  <% @lesson.errors.full_messages.each do |msg| %>
  <li><%= msg %></li>
 <% end %>
 </ul>
<% end %>

<%= form_for @lab do |f| %>

<p>
  <%= f.label :sessionNo %></br>
  <%= f.text_area :sessionNo %></br>
</p>
<p>
  <%= f.submit %>
 </p>
<% end %>

<%= link_to "Back to labs", labs_path %>