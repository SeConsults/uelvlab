<% if @activitylog.errors.any? %>
 <ul>
  <% @activitylog.errors.full_messages.each do |msg| %>
  <li><%= msg %></li>
 <% end %>
 </ul>
<% end %>
<%= form_for @activitylog do |f| %>
<p>
  <%= f.label :logNumber %></br>
  <%= f.text_area :logNunmber %>
</p>
<p>
  <%= f.submit %>
 </p>
<% end %>

<%= link_to "Back to Activitylog", activitylogs_path %>